{
  "name": "expresstest",
  "version": "0.0.1",
  "sideEffects": false,
  "private": true,
  "scripts": {
    "start": "npx ts-node src/app.ts",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/morgan": "^1.9.9",
    "@types/swagger-ui-express": "^4.1.6",
    "cookie-parser": "~1.4.4",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "http-errors": "~1.6.3",
    "js-yaml": "^4.1.0",
    "morgan": "~1.9.1",
    "pug": "^3.0.2",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.4.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "node-mocks": "^1.0.1",
    "node-mocks-http": "^1.14.1",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
